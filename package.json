{
  "name": "hariko",
  "version": "1.1.0",
  "description": "Mock Server that implements the API Blueprint specification.",
  "main": "index.js",
  "bin": {
    "hariko": "./bin/cli.js"
  },
  "scripts": {
    "start": "gulp",
    "test": "gulp test",
    "coverage": "istanbul cover _mocha 'test/**/*.js' --report lcovonly -- -R spec --no-color && cat ./coverage/lcov.info | codeclimate-test-reporter && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "bump": "mversion $1 -m '%s'"
  },
  "keywords": [
    "api",
    "api-blueprint",
    "hariko",
    "blueprint"
  ],
  "author": "mizuki_r <ry.mizuki@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.2",
    "colors": "^1.1.2",
    "dateformat": "^1.0.11",
    "express": "^4.13.1",
    "gaze": "^0.5.1",
    "glob": "^5.0.13",
    "http-proxy": "^1.11.1",
    "json5": "^0.4.0",
    "lodash": "^3.10.0",
    "mkdirp": "^0.5.1",
    "path-to-regexp": "^1.2.0",
    "protagonist": "^0.20.1",
    "yargs": "^3.15.0"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.1.0",
    "coffee-script": "^1.9.3",
    "conventional-changelog": "^0.1.3",
    "coveralls": "^2.11.2",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-jshint": "^1.11.2",
    "gulp-mocha": "^2.1.2",
    "istanbul": "^0.3.17",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "mversion": "^1.10.0",
    "require-subvert": "^0.1.0",
    "sinon": "^1.15.4",
    "supertest": "^1.0.1"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rymizuki/node-hariko.git"
  },
  "bugs": {
    "url": "https://github.com/rymizuki/node-hariko/issues"
  },
  "homepage": "https://github.com/rymizuki/node-hariko#readme"
}
